{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./",
    "rootDir": "./",
    "resolveJsonModule": true,
    "noEmitOnError": true
  },
  "include": ["main.ts"]
}